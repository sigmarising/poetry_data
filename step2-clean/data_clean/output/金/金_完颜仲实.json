    "dynasty": "金",
    "author": "完颜仲实",
    "intro": "",
    "items": [
        {
            "title": "朝中措",
            "content": [
                "襄阳古道灞陵桥，诗兴与秋高。",
                "千古风流人物，",
                "一时多少雄豪。",
                "霜清玉塞，云飞陇首，风落江皋。",
                "梦到凤凰台上，",
                "山围故国周遭。"
            ]
        }
    ]
}