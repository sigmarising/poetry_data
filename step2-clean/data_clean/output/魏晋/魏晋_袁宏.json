{
    "dynasty": "魏晋",
    "author": "袁宏",
    "intro": "",
    "items": [
        {
            "title": "拟古诗",
            "content": [
                "高馆百余仞。",
                "迢递虚中亭。",
                "文幌曜琼扇。",
                "碧疏映绮棂。"
            ]
        },
        {
            "title": "咏史诗",
            "content": [
                "无名困蝼蚁。",
                "有名世所疑。",
                "中庸难为体。",
                "狂狷不及时。",
                "杨恽非忌贵。",
                "知及有余辞。",
                "躬耕南山下。",
                "芜秽不遑治。",
                "赵瑟奏哀音。",
                "秦声歌新诗。",
                "吐音非凡唱。",
                "负此欲何之。"
            ]
        },
        {
            "title": "咏史诗",
            "content": [
                "周昌梗槩臣。",
                "辞达不为讷。",
                "汲黯社稷器。",
                "栋梁表天骨。",
                "陆贾厌解纷。",
                "时与酒梼杌。",
                "婉转将相门。",
                "一言和平勃。",
                "趋舍各有之。",
                "俱令道不没。"
            ]
        },
        {
            "title": "采菊诗",
            "content": [
                "息足回阿。",
                "圆坐长林。",
                "披榛即涧。",
                "藉草依阴。"
            ]
        },
        {
            "title": "从征行方头山诗",
            "content": [
                "峩峩太行。",
                "凌虚抗势。",
                "天岭交气。",
                "窈然无际。",
                "澄流入神。",
                "玄谷应契。",
                "四象悟心。",
                "幽人来憇"
            ]
        }
    ]
}