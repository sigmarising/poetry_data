{
    "dynasty": "南北",
    "author": "鲍令晖",
    "intro": "",
    "items": [
        {
            "title": "近代吴歌 欢闻",
            "content": [
                "遥遥天无柱。",
                "流漂萍无根。",
                "单身如萤火。",
                "持底报郎恩。"
            ]
        },
        {
            "title": "近代吴歌 上声",
            "content": [
                "留衫绣两裆。",
                "迮置罗裳里。",
                "微步动轻尘。",
                "罗衣随风起。"
            ]
        },
        {
            "title": "近代吴歌 前溪",
            "content": [
                "黄茑结蒙茏。",
                "生在洛溪边。",
                "花落逐流去。",
                "何见逐流还。"
            ]
        },
        {
            "title": "近代吴歌 冬歌",
            "content": [
                "渊冰厚三尺。",
                "素雪覆千里。",
                "我心如松柏。",
                "君心复何似。"
            ]
        },
        {
            "title": "近代吴歌 秋歌",
            "content": [
                "秋风入窗里。",
                "罗帐起飘扬。",
                "仰头看明月。",
                "寄情千里光。"
            ]
        },
        {
            "title": "近代吴歌 夏歌",
            "content": [
                "郁蒸仲暑月。",
                "长啸北湖边。",
                "芙蓉如结叶。",
                "拋艳未成莲。"
            ]
        },
        {
            "title": "近代吴歌 春歌",
            "content": [
                "朝日照北林。",
                "初花锦绣色。",
                "谁能春不思。",
                "独在机中织。"
            ]
        },
        {
            "title": "近代西曲歌 杨叛儿",
            "content": [
                "暂出白门前。",
                "杨柳可藏乌。",
                "郎作沈水香。",
                "侬作博山炉。"
            ]
        },
        {
            "title": "近代西曲歌 襄阳乐",
            "content": [
                "朝发襄阳城。",
                "暮至大堤宿。",
                "大堤诸女儿。",
                "花艳惊郎目。"
            ]
        },
        {
            "title": "近代西曲歌 乌夜啼",
            "content": [
                "歌舞诸年少。",
                "娉婷无穜迹。",
                "菖蒲花可怜。",
                "闻名不曾识。"
            ]
        },
        {
            "title": "近代西曲歌 估客乐",
            "content": [
                "有客数寄书。",
                "无信心相忆。",
                "莫作瓶落井。",
                "一去无消息。"
            ]
        },
        {
            "title": "近代西曲歌 石城乐",
            "content": [
                "生长石城下。",
                "开门对城楼。",
                "城中美年少。",
                "出入见依投。"
            ]
        },
        {
            "title": "寄行人",
            "content": [
                "桂吐两三枝。",
                "兰开四五叶。",
                "是时君不归。",
                "春风徒笑妾。"
            ]
        },
        {
            "title": "代葛沙门妻郭小玉诗",
            "content": [
                "君子将遥役。",
                "遗我双题锦。",
                "临当欲去时。",
                "复留相思枕。",
                "题用常着心。",
                "枕以忆同寝。",
                "行行日已远。",
                "转觉心弥甚。"
            ]
        },
        {
            "title": "自君之出矣",
            "content": [
                "自君之出矣。",
                "临轩不解颜。",
                "砧杵夜不发。",
                "高门昼常关。",
                "帐中流熠耀。",
                "庭前华紫兰。",
                "物枯识节异。",
                "鸿来知客寒。",
                "游用暮冬尽。",
                "除春待君还。"
            ]
        },
        {
            "title": "拟客从远方来",
            "content": [
                "客从远方来。",
                "赠我漆鸣琴。",
                "木有相思文。",
                "弦有别离音。",
                "终身执此调。",
                "岁寒不改心。",
                "愿作阳春曲。",
                "宫商长相寻。",
                "题书后寄行人"
            ]
        },
        {
            "title": "拟青青河畔草",
            "content": [
                "褭褭临窗竹。",
                "蔼蔼垂门桐。",
                "灼灼青轩女。",
                "泠泠高台中。",
                "明志逸秋霜。",
                "玉颜艳春红。",
                "人生谁不别。",
                "恨君早从戎。",
                "鸣弦惭夜月。",
                "绀黛羞春风。"
            ]
        },
        {
            "title": "钱唐苏小歌",
            "content": [
                "妾乘油璧车。",
                "郎骑青骢马。",
                "何处结同心。",
                "西陵松柏下。"
            ]
        },
        {
            "title": "丹阳孟珠歌",
            "content": [
                "阳春二三月。",
                "草与水同色。",
                "道逢游冶郎。",
                "恨不早相识。"
            ]
        },
        {
            "title": "近代杂歌 蚕丝歌",
            "content": [
                "春蚕不应老。",
                "昼夜常怀丝。",
                "何惜微躯尽。",
                "缠绵自有时。"
            ]
        },
        {
            "title": "近代杂歌 浔阳乐",
            "content": [
                "稽亭故人去。",
                "九里新人还。",
                "送一便迎两。",
                "无有暂时闲。"
            ]
        },
        {
            "title": "近代吴歌 独曲",
            "content": [
                "柳树得春风。",
                "一低复一昂。",
                "谁能空相忆。",
                "独眠度三阳。"
            ]
        },
        {
            "title": "近代吴歌 长乐佳",
            "content": [
                "红罗复斗帐。",
                "四角垂朱裆。",
                "玉枕龙须席。",
                "郎眠何处床。"
            ]
        },
        {
            "title": "近代杂歌 青阳歌曲",
            "content": [
                "青荷盖绿水。",
                "芙蓉发红鲜。",
                "下有并根藕。",
                "上生同心莲。"
            ]
        },
        {
            "title": "代葛沙门妻郭小玉诗",
            "content": [
                "明月何皎皎。",
                "垂櫎照罗茵。",
                "若共相思夜。",
                "知同忧怨晨。",
                "芳华岂矜貌。",
                "霜露不怜人。",
                "君非青云逝。",
                "飘迹事咸秦。",
                "妾持一生泪。",
                "经秋复度春。"
            ]
        },
        {
            "title": "近代杂诗",
            "content": [
                "玉钏色未分。",
                "衫轻似露腕。",
                "举袖欲障羞。",
                "回持理髪乱。"
            ]
        },
        {
            "title": "古意赠今人",
            "content": [
                "寒乡无异服，毡褐代文练。",
                "日月望君归，年年不解缓。",
                "荆扬春早和，幽冀犹霜霰。",
                "北寒妾已知，南心君不见。",
                "谁为道辛苦，寄情双飞燕。",
                "形迫抒煎丝，颜落风催电。",
                "容华一朝尽，惟余心不变。"
            ]
        }
    ]
}