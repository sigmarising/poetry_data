{
    "dynasty": "南北",
    "author": "颜延之",
    "intro": "",
    "items": [
        {
            "title": "向常侍",
            "content": [
                "向秀甘淡薄，",
                "深心托豪素。",
                "探道好渊玄，",
                "观书鄙章句。",
                "交吕既鸿轩，",
                "攀嵇亦凤举。",
                "流连河里游，",
                "恻怆山阳赋。"
            ]
        },
        {
            "title": "阮步兵",
            "content": [
                "阮公虽沦迹，",
                "识密鉴亦洞。",
                "沉醉似埋照，",
                "寓词类托讽。",
                "长啸若怀人，",
                "越礼自惊众。",
                "物故不可论，",
                "途穷能无恸。"
            ]
        },
        {
            "title": "嵇中散",
            "content": [
                "中散不偶世，",
                "本自餐霞人。",
                "形解验默仙，",
                "吐论知凝神。",
                "立俗迕流议，",
                "寻山洽隐沦。",
                "鸾翮有时铩，",
                "龙性谁能驯。"
            ]
        }
    ]
}